{
	"compilerOptions": {
		"diagnostics": true,
		"module": "commonjs",
		"target": "es5",
		"lib": ["dom", "es2015", "es2016", "es2017"],
		"sourceMap": true,
		"declaration": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitUseStrict": true,
		"noUnusedLocals": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"newLine": "LF"
	},
	"exclude": [
		"node_modules"
	]
}